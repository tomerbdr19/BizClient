    <Color x:Key="G00">#1D8649</Color>
    <Color x:Key="G10">#25A55A</Color>
    <Color x:Key="G20">#51B77B</Color>
    <Color x:Key="G30">#87CEA5</Color>
    <!--WARNING-->
    <Color x:Key="Y00">#D59900</Color>
    <Color x:Key="Y10">#FFB700</Color>
    <Color x:Key="Y20">#FFC23D</Color>
    <Color x:Key="Y30">#FFD16E</Color>
    <!--URGENT-->
    <Color x:Key="O00">#DF4D00</Color>
    <Color x:Key="O10">#FE620F</Color>
</ResourceDictionary>